{
  "name": "e2e-cypress-fov",
  "version": "1.0.0",
  "description": "This attempt made for E2E-CYPRESS-FOV, runnings cypress tests on FOV, FMM websites. Better generated a package.json and node_modules directory",
  "main": "index.js",
    "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:c:RFI_FR:d": "cypress run  --browser chrome --config video=false,screenshotOnRunFailure=false --env BRAND_LANG=\"RFI_FR\",DEVICE_TYPE=\"desktop\" --config-file \"config/RFI_FR.conf.env.cypress.config.js\"",
    "cy:run:c:RFI_FR:t": "cypress run  --browser chrome --config video=false,screenshotOnRunFailure=false --env BRAND_LANG=\"RFI_FR\",DEVICE_TYPE=\"tablet\" --config-file \"config/RFI_FR.conf.env.cypress.config.js\"",
    "cy:run:c:RFI_FR:m": "cypress run  --browser chrome --config video=false,screenshotOnRunFailure=false --env BRAND_LANG=\"RFI_FR\",DEVICE_TYPE=\"mobile\" --config-file \"config/RFI_FR.conf.env.cypress.config.js\"",
    "cy:run:c:RFI_FR:ml": "cypress run  --browser chrome --config video=false,screenshotOnRunFailure=false --env BRAND_LANG=\"RFI_FR\",DEVICE_TYPE=\"mobile-landscape\" --config-file \"config/RFI_FR.conf.env.cypress.config.js\"",
    "cy:run:c:RFI_FR:tl": "cypress run  --browser chrome --config video=false,screenshotOnRunFailure=false --env BRAND_LANG=\"RFI_FR\",DEVICE_TYPE=\"tablet-landscape\" --config-file \"config/RFI_FR.conf.env.cypress.config.js\"",
    "cy:run:c:F24_EN:d": "cypress run  --browser chrome --config video=false,screenshotOnRunFailure=false --env BRAND_LANG=\"F24_EN\",DEVICE_TYPE=\"desktop\" --config-file \"config/F24_EN.conf.env.cypress.config.js\"",
    "cy:run:c:F24_EN:t": "cypress run  --browser chrome --config video=false,screenshotOnRunFailure=false --env BRAND_LANG=\"F24_EN\",DEVICE_TYPE=\"tablet\" --config-file \"config/F24_EN.conf.env.cypress.config.js\"",
    "cy:run:c:F24_EN:m": "cypress run  --browser chrome --config video=false,screenshotOnRunFailure=false --env BRAND_LANG=\"F24_EN\",DEVICE_TYPE=\"mobile\" --config-file \"config/F24_EN.conf.env.cypress.config.js\"",
    "cy:run:c:F24_EN:ml": "cypress run  --browser chrome --config video=false,screenshotOnRunFailure=false --env BRAND_LANG=\"F24_EN\",DEVICE_TYPE=\"mobile-landscape\" --config-file \"config/F24_EN.conf.env.cypress.config.js\"",
    "cy:run:c:F24_EN:tl": "cypress run  --browser chrome --config video=false,screenshotOnRunFailure=false --env BRAND_LANG=\"F24_EN\",DEVICE_TYPE=\"tablet-landscape\" --config-file \"config/F24_EN.conf.env.cypress.config.js\""
  },
  "keywords": [
    "cypress",
    "attempt",
    "E2E-CYPRESS-FOV",
    "e2e-cypress-fov",
    "fov"
  ],
  "author": "bflaven",
  "license": "ISC",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.34.0",
    "cypress": "^12.9.0",
    "cypress-failed-log": "^2.10.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-grep": "^2.6.1",
    "cypress-multi-reporters": "^1.6.2",
    "cypress-real-events": "^1.7.6",
    "cypress-wait-until": "^1.7.2",
    "cypress-web-vitals": "^2.2.1",
    "cypress-xpath": "^1.8.0",
    "debug": "^4.3.3",
    "faker": "^5.5.3",
    "fs-extra": "^10.0.1",
    "mocha": "^9.2.2",
    "mocha-allure-reporter": "^1.4.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "mochawesome-report-generator": "^6.2.0",
    "path": "^0.12.7",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "cypress-parallel": "^0.11.0",
    "fs": "^0.0.1-security",
    "git-auto-pull": "^1.0.9"
  },
  "repository": {
    "type": "git",
